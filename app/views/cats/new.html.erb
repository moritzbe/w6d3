<h1> Create Cat </h1>
<%= form_for [@cat] do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name %>
      <br>
      <%= f.label :birthday %>
      <%= f.text_field :birthday %>
      <br>
      <%= f.label :password_digest %>
      <%= f.text_field :password_digest %>
      <br>
      <%= f.label :email %>
      <%= f.text_field :email %>
      <br>
      <%= f.submit 'Save' %>
<%end%>